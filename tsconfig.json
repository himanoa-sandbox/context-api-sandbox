{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "esnext",
    "module": "es2015",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "lib": [
      "esnext",
      "dom"
    ],
    "typeRoots": ["types", "node_modules/@types"],
    "types": [],
    "paths": {},
    "jsx": "react"
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
